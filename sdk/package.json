{
  "name": "@simplyprint/nfc-agent",
  "version": "0.4.1",
  "description": "JavaScript SDK for NFC Agent - read and write NFC cards via local nfc-agent server",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "nfc",
    "rfid",
    "ntag",
    "mifare",
    "smartcard",
    "reader",
    "simplyprint"
  ],
  "author": "SimplyPrint",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SimplyPrint/nfc-agent.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/SimplyPrint/nfc-agent/issues"
  },
  "homepage": "https://github.com/SimplyPrint/nfc-agent#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.8",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
